				</textElement>
				<text><![CDATA[TITRE II- CONVENTIONS]]></text>
			</staticText>
			<staticText>
				<reportElement x="1" y="293" width="66" height="10" uuid="780fb002-2b55-4c44-8789-5ac04f128d4f"/>
				<textElement verticalAlignment="Top">
					<font fontName="Verdana" size="8" isBold="true" isUnderline="true"/>
